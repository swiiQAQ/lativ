function formatArray(num){
  var arr = [];
  for(var i = 0; i < num; i++){
    arr.push(i);
  }
  return arr;
}

function indexOf(arr,item){
  console.log("arr:"+arr);
  console.log("item:"+item);
  if(arr.indexOf(item) == -1){
    return false;
  }
  else{
    return true;
  }
}

module.exports={
  formatArray: formatArray,
  indexOf : indexOf
}